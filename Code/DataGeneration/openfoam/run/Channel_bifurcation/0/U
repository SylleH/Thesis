FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
    version	9;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    bottom
    {
        type            zeroGradient;
    }

    left
    {
        type            noSlip;
    }
    
    top
    {
	type		uniformFixedValue;
    	uniformValue
    	{
		type			tableFile;
		format			csv;
		nHeaderLine		1;
		refColumn		0;
		componentColumns	(1 2 3);
		separator		",";
		mergeSeparators		no;
        	file			"inlet_v.csv";
    	}
    }

    right
    {
        type            noSlip;
    }
    
    inner_arc
    {
        type            noSlip;
    }
}

