FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0.29";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    bottom
    {
        type            uniformFixedValue;
        uniformValue    
        {
            type            scale;
            scale           
            {
                type            squarePulse;
                start           0.3;
                duration        0.3;
            }
            xScale          
            {
                type            constant;
                value           1;
            }
            value           
            {
                type            constant;
                value           15.1;
            }
        }
        value           uniform 0;
    }
    left
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    inner_arc
    {
        type            zeroGradient;
    }
}


