FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    bottom
    {
        type            zeroGradient;
    }


    outer_arc
    {
        type		zeroGradient;
    }

    right
    {
        type         uniformFixedValue;
    	uniformValue
    	{
        	type            scale;
		scale		squarePulse;
		start		0.3;	
        	duration	0.3;
        	value        	15.1;
    	}
    }
    
    inner_arc
    {
        type            zeroGradient;
    }
}

