boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    top
    {
        type            codedFixedValue;
        value           nonuniform List<vector> 10((0 -0.0630157 0) (0 -0.0685555 0) (0 -0.051936 0) (0 -0.0353165 0) (0 -0.0353165 0) (0 -0.051936 0) (0 -0.0630157 0) (0 -0.0131571 0) (0 -0.0131571 0) (0 -0.0685555 0));
        name            ParabolicTimeDependentU_max;
        code            #{
		const vectorField& Cf = patch().Cf();
		vectorField& field = *this;

		const scalar r = 12.0*1e-3;
		const scalar Umax = 0.4;
		const scalar Umin = 0.02;

		scalar 	Uvar = 0.08;
		scalar 	t = this->db().time().value();
		scalar	tau = fmod(t,1);
		
		if (tau > 0 && tau <= 0.35)
		{
			Uvar = 0.08+Umax*(1-(pow(tau-0.175,2)/(0.175*0.175)));
		}
		if (tau > 0.35 && tau <= 0.6)
		{
			Uvar = 0.08+Umin*(-1+(pow(tau-0.475,2)/(0.125*0.125)));
		}
		if (tau > 0.6 && tau <= 1)
		{
			Uvar = 0.08;
		}
		
		forAll(Cf, faceI)
		{
			const scalar x = Cf[faceI][0];
			const scalar y = Cf[faceI][1];
			field[faceI] = vector(0, Uvar*(-1+(pow(x+(0.02+r),2))/(r*r)), 0);
		}
	#};
    }
    outer_arc
    {
        type            noSlip;
    }
    bottom
    {
        type            zeroGradient;
    }
    inner_arc
    {
        type            noSlip;
    }
}


